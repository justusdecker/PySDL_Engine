
all:
	gcc -Wall -Werror -c ./src/wsdl.c -I src/include -L src/lib -l./bin/SDL3 -o ./bin/wsdl.o
	gcc -Wall -Werror -shared -o ./bin/wsdl.dll ./bin/wsdl.o -L src/lib -lSDL3
	python ./tools/zipit.py ./test.zip ./bin/wsdl.dll ./bin/wsdl.o
run:
	./main.exe
clean:
	rm ./bin/wsdl.dll
	rm ./bin/wsdl.o
push:
	call ./tools/action_push.bat
	rm ./tools/cm.i
	rm ./tools/cmtmsg.i
	rm ./tools/hash.i